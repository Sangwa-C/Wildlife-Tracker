{{#partial "content"}}



    <div class="well" style="text-align:center" >
        <small style="font-size:1em; text-align:center"> ~No effort to save these animals is too small~ </small>
        {{#if animals}}
            {{#each animals}}

                <p> Animal's name: {{ name }} </p>
            {{/each}}
        {{/if}}
    </div>


        {{#if ibinabyo}}
            {{#each ibinabyo}}

                <div class="well" style="text-align:center">
                    <small style="font-size:1em; text-align:center"> ~No effort to save these animals is too small~ </small>
                    <p> Animal's name: {{aname}} </p>
                    <p> Animal's health: {{health}} </p>
                    <p> Animal's age: {{age}} </p>
                    <p> Animal's location: {{location}} </p>
                    <p> Ranger Name: {{rangename}} </p>
                </div>




            {{/each}}
        {{/if}}





{{/partial}}

{{> Layout.hbs}}